	text-align: center;
	color: white;
	font-family: Noto Sans;
	font-size: 25px;
}

.foottext{
	color: red;
	text-align: center;
}

.div-text{
	width: 100vw;
	height: 20vh;
	text-align: center;
	max-width: 100%;
}

.div-img{
	width: auto;
	height: 35vh;
	text-align: center;
	background-color: solid black;
	max-width: 100%;
}

#image-bg{
	width: 80vw;
}

.acasa{
	color: white;
	float: left;
	width: 20%;
	height: 5vh;
}

#acasa-img{
	width: 15vw;
}

.despre{
	color: white;
	font-size: 25px;
	float: left;
	width: 55%;
}

#despre-img{
	width: 15vw;
}

.contact{
	color: white;
	font-size:25px;
	float: left;
	width:24%;
}

#contact-img{
	width: 15vw;
}

.div-nav{
	padding: 3%;
	color: white;
	height: 6vh;
	text-align: center;
	font-family: Noto Sans;
	background-color: solid black;
	max-width: 100%;
}

.GeneratedMarquee {
	line-height: 5em;
	height: 9em;
	

}

.foottext{
	position: absolute;
	bottom: 10px;
	left: 10px;
	right: 10px;
}

form{
	margin: 0 auto;
	width: 400px;
	padding: 1em;
	border: 1px solid #CCC;
	border-radius: 1em;
}
ul{
	list-style: none;
	padding: 0;
	margin: 0;
}

form li + li {
	margin-top: 0.5em;
}

input,
textarea {
	width: 280px;
	box-sizing: border-box;
	border: 1px solid #999;
}

input: focus
textarea: focus {
	border-color: #000;
}

textarea {
	vertical-align: top;
	height: 5em;
}

label{
	color: white;
	display: inline-block;
	width: 90px;
	text-align: center;
	font-family: Noto Sans;
}

.button_contact{
	text-align: center;
	padding-left: 60px;
}

.continut_contact{
	color: white;
	text-align: center;
	margin: 0 auto;
	height: 30vh;
	margin-top: 100px;
	font-size: 10;
	font-family: Noto Sans;
	vertical-align: top;
	display: inline-block;
}
.continut_contact
li+ li {
	margin-top: 0.5em;
}